{
  "name": "foodai",
  "private": true,
  "scripts": {
    "predev": "node -e \"const {execSync}=require('child_process'); try{execSync('lsof -ti:3001 | xargs kill -9',{stdio:'ignore'})}catch{};\"",
    "dev": "cross-env PORT=3001 NEXT_TELEMETRY_DISABLED=1 NEXT_DISABLE_HTTP_CACHE=1 next dev",
    "build": "NEXT_OUTPUT_STANDALONE=true next build",
    "start": "next start -p 3001",
    "dev:clean": "rm -rf .next && npm run dev",
    "kill:ports": "lsof -ti:3000,3001,3002,3003,3004,3005 | xargs kill -9 || true",
    "snapshot": "git add -A && git commit -m \"snapshot: ui checkpoint\" && git tag -f ui-last && git describe --tags --always",
    "restore:template": "git reset --hard template-v1 && git clean -xfd && npm i && npm run dev",
    "restore": "git reset --hard ui-last"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-leaflet/core": "2.1.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.5.0",
    "@supabase/supabase-js": "^2.45.0",
    "@types/leaflet": "1.9.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parse": "^5.5.6",
    "dotenv": "^17.2.2",
    "leaflet": "1.9.4",
    "lodash": "4.17.21",
    "lucide-react": "^0.441.0",
    "next": "14.2.32",
    "next-intl": "^3.26.5",
    "next-themes": "^0.4.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-leaflet": "4.2.1",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.16",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^10.0.0",
    "eslint": "^9.35.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.1"
  }
}

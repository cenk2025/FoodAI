# .env.local.example
NEXT_PUBLIC_SITE_URL=http://localhost:3001
NEXT_PUBLIC_DEFAULT_LOCALE=fi

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE=your_supabase_service_role_key_here

# DeepSeek AI
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Cron job token for admin endpoints
CRON_TOKEN=your_secret_cron_token

# Commissions webhook secret
COMMISSIONS_WEBHOOK_SECRET=your_webhook_secret

# Optional: Google OAuth in Supabase (Auth providers > Google)
NEXT_PUBLIC_GOOGLE_ENABLED=false
